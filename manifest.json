{
    "manifest_version": 3,
    "name": "Quicktext",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon": {
            "16": "./images/monkey.png",
            "32": "./images/monkey.png",
            "48": "./images/monkey.png",
            "128": "./images/monkey.png"
          }
    }
}
